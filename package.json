{
  "name": "bubble-sort-visualizer",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "start": "parcel index.html",
    "build": "parcel build index.html --public-url /bubble-sort-visualizer/",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d dist"
  },
  "homepage": "http://ashwanth1109.github.io/bubble-sort-visualizer",
  "license": "MIT",
  "dependencies": {
    "gh-pages": "^2.1.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "babel-preset-nano-react-app": "^0.1.0",
    "parcel-bundler": "^1.12.3"
  },
  "babel": {
    "presets": [
      "nano-react-app"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ],
      [
        "@babel/plugin-transform-react-jsx",
        {
          "pragmaFrag": "React.Fragment"
        }
      ]
    ]
  },
  "main": "index.js",
  "repository": "https://github.com/ashwanth1109/bubble-sort-visualizer.git",
  "author": "Ashwanth A R <ashwanth1109@gmail.com>"
}
